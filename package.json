        "prettier": "^3.2.5",
        "tsup": "^8.0.2",
        "tsx": "^4.9.3",
        "typedoc": "^0.25.13",
        "typescript": "^5.4.5",
        "vitest": "^2.1.8",
        "winston": "^3.17.0"
    },
    "dependencies": {
